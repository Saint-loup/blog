{
  "name": "tout-ce-qui-bouge",
  "version": "0.2.0",
  "scripts": {
    "clean": "rimraf dist",
    "dev:webpack": "webpack  --mode development",
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss src/assets/css/main.css -o dist/assets/css/main.css --watch",
    "dev": "npm-run-all clean --parallel dev:* --print-label",
    "build:webpack": "webpack  --mode production",
    "build:eleventy": "cross-env NODE_ENV=production eleventy",
    "build:postcss": "cross-env NODE_ENV=production postcss src/assets/css/main.css -o dist/assets/css/main.css",
    "build": "run-s clean build:* --print-label"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.8.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.13.15",
    "@sindresorhus/slugify": "1.1.0",
    "@tailwindcss/forms": "^0.3.2",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eleventy-plugin-embed-everything": "^1.13.1",
    "eleventy-plugin-images-responsiver": "^1.8.2",
    "full-icu": "^1.3.1",
    "html-minifier": "^4.0.0",
    "js-yaml": "^3.14.0",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.1",
    "markdown-it-anchor": "^7.0.2",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-image-figures": "^1.0.1",
    "markdown-it-implicit-figures": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.2.2",
    "postcss-hash": "^2.0.0",
    "postcss-import": "^14.0.1",
    "postcss-loader": "^5.2.0",
    "postcss-nested": "^5.0.3",
    "remove-markdown": "^0.3.0",
    "rimraf": "^3.0.2",
    "tailwindcss-textshadow": "^2.1.3",
    "webpack": "^5.33.2",
    "webpack-assets-manifest": "^5.0.6",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "alpinejs": "^2.8.0",
    "elasticlunr": "github:weixsong/elasticlunr.js#v0.9.6",
    "photoswipe": "^4.1.3",
    "pretty-data": "^0.40.0",
    "tailwindcss": "^2.1.1"
  },
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": "> 0.25%, not dead"
      }
    ]
  ]
}