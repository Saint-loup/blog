{
  "name": "tout-ce-qui-bouge",
  "version": "1.1.2",
  "scripts": {
    "clean": "rimraf dist",
    "dev:webpack": "webpack  --mode development",
    "dev:eleventy": "  eleventy --serve",
    "dev:postcss": "postcss src/assets/css/tailwind.css -o dist/assets/tailwind.css --watch",
    "dev": " npm-run-all clean --parallel dev:* --print-label",
    "build:webpack": "webpack  --mode production",
    "build:eleventy": " cross-env ELEVENTY_ENV=production eleventy",
    "build:postcss": " cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o dist/assets/tailwind.css",
    "build": "run-s clean build:* --print-label"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.4",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@tailwindcss/custom-forms": "^0.2.1",
    "alpinejs": "^2.8.0",
    "autoprefixer": "^10.2.4",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "clean-css": "^4.2.3",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eleventy-plugin-page-assets": "^0.3.0",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^11.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-cli": "^8.2.2",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^5.0.3",
    "rimraf": "^3.0.2",
    "slugify": "^1.4.6",
    "tailwindcss": "^2.0.3",
    "tailwindcss-textshadow": "^2.1.3",
    "terser-webpack-plugin": "^4.2.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^0.8.0",
    "@sindresorhus/slugify": "^1.1.0",
    "@tailwindcss/typography": "^0.4.0",
    "js-yaml": "^3.14.0",
    "markdown-it-anchor": "^7.0.2",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-implicit-figures": "^0.10.0",
    "moment": "^2.29.1"
  }
